<?xml version="1.0" encoding="utf-8"?>
<com.medlinker.hybrid.widget.NativeLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <com.medlinker.hybrid.widget.NativeButton
        android:id="@+id/change_screen"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="切换横竖屏" />

    <TextView
        android:id="@+id/text_screen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hello World, MyActivity" />

    <com.medlinker.hybrid.widget.MySurfaceView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
</com.medlinker.hybrid.widget.NativeLinearLayout>